def dfs(src,par,vis,adj):
        vis[src]=1
        for i in adj[src]:
            if vis[i]==-1:
                return dfs(i,src,vis,adj)
            elif i!=par:
                return True
        return False
def isCycle( V, adj) -> bool:
    vis=[-1]*V
    for i in range(V):
     if vis[i] ==-1:
         if dfs(i,-1,vis,adj)==False:
             pass
         else:
             return True
    return False
adj = [[1], [0, 2, 4], [1, 3], [2, 4], [1, 3]] 
print(isCycle(5,adj))